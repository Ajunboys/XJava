<?xml version="1.0" encoding="UTF-8"?>

<config>

	<import name="uceConfig"   class="java.util.ArrayList" />
	<import name="item"        class="org.hy.common.app.Param" />
	<import name="expressions" class="org.hy.common.TablePartitionRID" />


	<!-- 单位转换的表达式配置 -->
	<uceConfig>
		
		<item id="UnitConvertPrecision">
			<name>UnitConvertPrecision</name>
			<value>9</value>
			<comment>单位转换的精度。默认的保留小数的位数</comment>
		</item>
		
		
    
		<expressions id="UnitConvertExpressions" this="UnitConvertExpressions">
			
			<!-- 温度单位 -->
			<call name="putRow">
				<String>摄氏度</String>
				<String>华氏度</String>
				<String>UCE * 9 / 5 + 32</String>
			</call>
			
			<call name="putRow">
				<String>°C</String>
				<String>°F</String>
				<String>UCE * 9 / 5 + 32</String>               <!-- 摄氏度 to 华氏度 -->
			</call>
			
			<call name="putRow">
				<String>华氏度</String>
				<String>摄氏度</String>
				<String>(UCE - 32) * 5 / 9</String>
			</call>
			
			<call name="putRow">
				<String>°F</String>
				<String>°C</String>
				<String>(UCE - 32) * 5 / 9</String>             <!-- 华氏度 to 摄氏度 -->
			</call>
			
			<call name="putRow">
				<String>摄氏度</String>
				<String>开氏度</String>
				<String>UCE + 273.15</String>
			</call>
			
			<call name="putRow">
				<String>°C</String>
				<String>K</String>
				<String>UCE + 273.15</String>                   <!-- 摄氏度 to 开氏度 -->
			</call>
			
			<call name="putRow">
				<String>开氏度</String>
				<String>摄氏度</String>
				<String>UCE - 273.15</String>
			</call>
			
			<call name="putRow">
				<String>K</String>
				<String>°C</String>
				<String>UCE - 273.15</String>                   <!-- 开氏度 to 摄氏度 -->
			</call>
			
			
			
			<!-- 长度单位 -->
			<call name="putRow">
				<String>毫米</String>
				<String>米</String>
				<String>UCE * 0.001</String>
			</call>
			
			<call name="putRow">
				<String>mm</String>
				<String>m</String>
				<String>UCE * 0.001</String>                    <!-- 毫米 to 米 -->
			</call>
			
			<call name="putRow">
				<String>米</String>
				<String>毫米</String>
				<String>UCE * 1000</String>
			</call>
			
			<call name="putRow">
				<String>m</String>
				<String>mm</String>
				<String>UCE * 1000</String>                     <!-- 米 to 毫米 -->
			</call>
			
			
			
			<!-- 压力单位：绝压to表压的部 -->
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>兆帕(表压)</String>
				<String>UCE - 0.101325</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>MPa.g</String>
				<String>UCE - 0.101325</String>                 <!-- 兆帕(绝压) to 兆帕(表压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(表压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE + 0.101325</String>
			</call>
			
			<call name="putRow">
				<String>MPa.g</String>
				<String>MPa.a</String>
				<String>UCE + 0.101325</String>                 <!-- 兆帕(表压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>巴(表压)</String>
				<String>UCE * 10 - 1.01325</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>bar.g</String>
				<String>UCE * 10 - 1.01325</String>             <!-- 兆帕(绝压) to 巴(表压) -->
			</call>
			
			<call name="putRow">
				<String>巴(表压)</String>
				<String>兆帕(绝压)</String>
				<String>(UCE + 1.01325) / 10</String>
			</call>
			
			<call name="putRow">
				<String>bar.g</String>
				<String>MPa.a</String>
				<String>(UCE + 1.01325) / 10</String>           <!-- 巴(表压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>千克力/平方厘米(表压)</String>
				<String>UCE * 10.1972 - 1.01325</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>kgf/cm^2.g</String>
				<String>UCE * 10.1972 - 1.01325</String>        <!-- 兆帕(绝压) to 千克力/平方厘米(表压) -->
			</call>
			
			<call name="putRow">
				<String>千克力/平方厘米(表压)</String>
				<String>兆帕(绝压)</String>
				<String>(UCE + 1.01325) / 10.1972</String>
			</call>
			
			<call name="putRow">
				<String>kgf/cm^2.g</String>
				<String>MPa.a</String>
				<String>(UCE + 1.01325) / 10.1972</String>      <!-- 千克力/平方厘米(表压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>标准大气压(表压)</String>
				<String>UCE * 9.86923 - 1.01325</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>atm.g</String>
				<String>UCE * 9.86923 - 1.01325</String>        <!-- 兆帕(绝压) to 标准大气压(表压) -->
			</call>
			
			<call name="putRow">
				<String>标准大气压(表压)</String>
				<String>兆帕(绝压)</String>
				<String>(UCE + 1.01325) / 9.86923</String>
			</call>
			
			<call name="putRow">
				<String>atm.g</String>
				<String>MPa.a</String>
				<String>(UCE + 1.01325) / 9.86923</String>      <!-- 标准大气压(表压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>磅力/平方英寸(表压)</String>
				<String>(UCE - 0.101325) * 145.036</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>psi.g</String>
				<String>(UCE - 0.101325) * 145.036</String>     <!-- 兆帕(绝压) to 磅力/平方英寸(表压) -->
			</call>
			
			<call name="putRow">
				<String>磅力/平方英寸(表压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 145.036 + 0.101325</String>
			</call>
			
			<call name="putRow">
				<String>psi.g</String>
				<String>MPa.a</String>
				<String>UCE / 145.036 + 0.101325</String>       <!-- 磅力/平方英寸(表压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>毫米汞柱(0°C)(表压)</String>
				<String>(UCE - 0.101325) * 7500.62</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>mmHg.g</String>
				<String>(UCE - 0.101325) * 7500.62</String>     <!-- 兆帕(绝压) to 毫米汞柱(0°C)(表压) -->
			</call>
			
			<call name="putRow">
				<String>毫米汞柱(0°C)(表压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 7500.62 + 0.101325</String>
			</call>
			
			<call name="putRow">
				<String>mmHg.g</String>
				<String>MPa.a</String>
				<String>UCE / 7500.62 + 0.101325</String>       <!-- 毫米汞柱(0°C)(表压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>毫米水柱(15°C)(表压)</String>
				<String>(UCE - 0.101325) * 102074</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>mmH2O.g</String>
				<String>(UCE - 0.101325) * 102074</String>      <!-- 兆帕(绝压) to 毫米水柱(15°C)(表压) -->
			</call>
			
			<call name="putRow">
				<String>毫米水柱(15°C)(表压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 102074 + 0.101325</String>
			</call>
			
			<call name="putRow">
				<String>mmH2O.g</String>
				<String>MPa.a</String>
				<String>UCE / 102074 + 0.101325</String>        <!-- 毫米水柱(15°C)(表压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>帕斯卡(表压)</String>
				<String>(UCE - 0.101325) * 1000000</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>Pa.g</String>
				<String>(UCE - 0.101325) * 1000000</String>     <!-- 兆帕(绝压) to 帕斯卡(表压) -->
			</call>
			
			<call name="putRow">
				<String>帕斯卡(表压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 1000000 + 0.101325</String>
			</call>
			
			<call name="putRow">
				<String>Pa.g</String>
				<String>MPa.a</String>
				<String>UCE / 1000000 + 0.101325</String>       <!-- 帕斯卡(表压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>千帕(表压)</String>
				<String>(UCE - 0.101325) * 1000</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>KPa.g</String>
				<String>(UCE - 0.101325) * 1000</String>        <!-- 兆帕(绝压) to 千帕(表压) -->
			</call>
			
			<call name="putRow">
				<String>千帕(表压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 1000 + 0.101325</String>
			</call>
			
			<call name="putRow">
				<String>KPa.g</String>
				<String>MPa.a</String>
				<String>UCE / 1000 + 0.101325</String>          <!-- 千帕(表压) to 兆帕(绝压) -->
			</call>
			
			
			
			<!-- 压力单位：绝压to绝压的部 -->
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>巴(绝压)</String>
				<String>UCE * 10</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>bar.a</String>
				<String>UCE * 10</String>                       <!-- 兆帕(绝压) to 巴(绝压) -->
			</call>
			
			<call name="putRow">
				<String>巴(绝压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 10</String>
			</call>
			
			<call name="putRow">
				<String>bar.a</String>
				<String>MPa.a</String>
				<String>UCE / 10</String>                       <!-- 巴(绝压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>千克力/平方厘米(绝压)</String>
				<String>UCE * 10.1972</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>kgf/cm^2.a</String>
				<String>UCE * 10.1972</String>                  <!-- 兆帕(绝压) to 千克力/平方厘米(绝压) -->
			</call>
			
			<call name="putRow">
				<String>千克力/平方厘米(绝压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 10.1972</String>
			</call>
			
			<call name="putRow">
				<String>kgf/cm^2.a</String>
				<String>MPa.a</String>
				<String>UCE / 10.1972</String>                  <!-- 千克力/平方厘米(绝压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>标准大气压(绝压)</String>
				<String>UCE * 9.86923</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>atm.a</String>
				<String>UCE * 9.86923</String>                  <!-- 兆帕(绝压) to 标准大气压(绝压) -->
			</call>
			
			<call name="putRow">
				<String>标准大气压(绝压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 9.86923</String>
			</call>
			
			<call name="putRow">
				<String>atm.a</String>
				<String>MPa.a</String>
				<String>UCE / 9.86923</String>                  <!-- 标准大气压(绝压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>磅力/平方英寸(绝压)</String>
				<String>UCE * 145.036</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>psi.a</String>
				<String>UCE * 145.036</String>                  <!-- 兆帕(绝压) to 磅力/平方英寸(绝压) -->
			</call>
			
			<call name="putRow">
				<String>磅力/平方英寸(绝压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 145.036</String>
			</call>
			
			<call name="putRow">
				<String>psi.a</String>
				<String>MPa.a</String>
				<String>UCE / 145.036</String>                  <!-- 磅力/平方英寸(绝压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>毫米汞柱(0°C)(绝压)</String>
				<String>UCE * 7500.62</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>mmHg.a</String>
				<String>UCE * 7500.62</String>                  <!-- 兆帕(绝压) to 毫米汞柱(0°C)(绝压) -->
			</call>
			
			<call name="putRow">
				<String>毫米汞柱(0°C)(绝压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 7500.62</String>
			</call>
			
			<call name="putRow">
				<String>mmHg.a</String>
				<String>MPa.a</String>
				<String>UCE / 7500.62</String>                  <!-- 毫米汞柱(0°C)(绝压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>毫米水柱(15°C)(绝压)</String>
				<String>UCE * 102074</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>mmH2O.a</String>
				<String>UCE * 102074</String>                   <!-- 兆帕(绝压) to 毫米水柱(15°C)(绝压) -->
			</call>
			
			<call name="putRow">
				<String>毫米水柱(15°C)(绝压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 102074</String>
			</call>
			
			<call name="putRow">
				<String>mmH2O.a</String>
				<String>MPa.a</String>
				<String>UCE / 102074</String>                   <!-- 毫米水柱(15°C)(绝压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>帕斯卡(绝压)</String>
				<String>UCE * 1000000</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>Pa.a</String>
				<String>UCE * 1000000</String>                  <!-- 兆帕(绝压) to 帕斯卡(绝压) -->
			</call>
			
			<call name="putRow">
				<String>帕斯卡(绝压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 1000000</String>
			</call>
			
			<call name="putRow">
				<String>Pa.a</String>
				<String>MPa.a</String>
				<String>UCE / 1000000</String>                  <!-- 帕斯卡(绝压) to 兆帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>兆帕(绝压)</String>
				<String>千帕(绝压)</String>
				<String>UCE * 1000</String>
			</call>
			
			<call name="putRow">
				<String>MPa.a</String>
				<String>KPa.a</String>
				<String>UCE * 1000</String>                     <!-- 兆帕(绝压) to 千帕(绝压) -->
			</call>
			
			<call name="putRow">
				<String>千帕(绝压)</String>
				<String>兆帕(绝压)</String>
				<String>UCE / 1000</String>
			</call>
			
			<call name="putRow">
				<String>KPa.a</String>
				<String>MPa.a</String>
				<String>UCE / 1000</String>                     <!-- 千帕(绝压) to 兆帕(绝压) -->
			</call>
			
			
			
			<!-- 粘度单位 -->
			<call name="putRow">
				<String>毫帕斯卡.秒</String>
				<String>厘泊</String>
				<String>UCE</String>
			</call>
			
			<call name="putRow">
				<String>mPa.s</String>
				<String>cP</String>
				<String>UCE</String>                            <!-- 别名：毫帕斯卡.秒 to 厘泊 -->
			</call>
			
			<call name="putRow">
				<String>厘泊</String>
				<String>毫帕斯卡.秒</String>
				<String>UCE</String>
			</call>
			
			<call name="putRow">
				<String>cP</String>
				<String>mPa.s</String>
				<String>UCE</String>                            <!-- 别名：厘泊 to 毫帕斯卡.秒 -->
			</call>
			
			<call name="putRow">
				<String>平方毫米/秒</String>
				<String>厘斯</String>
				<String>UCE</String>
			</call>
			
			<call name="putRow">
				<String>mm^2/s</String>
				<String>cST</String>
				<String>UCE</String>                            <!-- 别名：平方毫米/秒 to 厘斯 -->
			</call>
			
			<call name="putRow">
				<String>厘斯</String>
				<String>平方毫米/秒</String>
				<String>UCE</String>
			</call>
			
			<call name="putRow">
				<String>cST</String>
				<String>mm^2/s</String>
				<String>UCE</String>                            <!-- 别名：厘斯 to 平方毫米/秒 -->
			</call>
			
			<call name="putRow">
				<String>平方毫米/秒</String>
				<String>平方米/秒</String>
				<String>UCE / 1000000</String>
			</call>
			
			<call name="putRow">
				<String>mm^2/s</String>
				<String>m^2/s</String>
				<String>UCE / 1000000</String>                  <!-- 平方毫米/秒 to 平方米/秒 -->
			</call>
			
			<call name="putRow">
				<String>平方米/秒</String>
				<String>平方毫米/秒</String>
				<String>UCE * 1000000</String>
			</call>
			
			<call name="putRow">
				<String>m^2/s</String>
				<String>mm^2/s</String>
				<String>UCE * 1000000</String>                  <!-- 平方米/秒 to 平方毫米/秒 -->
			</call>
			
			<call name="putRow">
				<String>平方毫米/秒</String>
				<String>平方米/小时</String>
				<String>UCE / 277.777</String>
			</call>
			
			<call name="putRow">
				<String>mm^2/s</String>
				<String>m^2/h</String>
				<String>UCE / 277.777</String>                  <!-- 平方毫米/秒 to 平方米/小时 -->
			</call>
			
			<call name="putRow">
				<String>平方米/小时</String>
				<String>平方毫米/秒</String>
				<String>UCE * 277.777</String>
			</call>
			
			<call name="putRow">
				<String>m^2/h</String>
